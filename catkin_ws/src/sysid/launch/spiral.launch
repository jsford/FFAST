<launch>

    <node
        name="teensy" pkg="rosserial_python" type="serial_node.py" respawn="true" output="screen">
        <param name="port" value="/dev/teensy" />
        <param name="baud" value="115200" />
    </node>

    <node
        name="vmu931" pkg="vmu931" type="vmu931_node" respawn="true" output="screen">
        <param name="serial_port" value="/dev/VMU931" />
    </node>

    <node name="spiral" pkg="sysid" type="spiral" required="true" output="screen" />

</launch>
